<jpd:jqassistant-plugin xmlns:jpd="http://schema.jqassistant.org/plugin/v1.10"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://schema.jqassistant.org/plugin/v1.10 https://schema.jqassistant.org/plugin/jqassistant-plugin-v1.10.xsd"
                        name="GraphQL" id="jqa.plugin.graphql">
    <description>Provides a scanner for GraphQL schemas.</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.SchemaFileDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.SchemaUrlDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.DirectiveTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.DirectiveLocationDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.DirectiveValueDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.EnumTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.EnumValueDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.ScalarTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.ScalarValueDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.InputObjectTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.InterfaceTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.UnionTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.UnionDeclaresTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.InputFieldDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.InputValueDefinitionDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.ListTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.FieldOfTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.ArgumentDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.InputValueOfTypeDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.graphql.api.model.OfElementTypeDescriptor</class>
    </model>
    <scope>
        <class>com.buschmais.jqassistant.plugin.graphql.scope.GraphQLScope</class>
    </scope>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.graphql.impl.scanner.GraphQLSchemaFileScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.graphql.impl.scanner.GraphQLSchemaURLScannerPlugin</class>
        <class>com.buschmais.jqassistant.plugin.graphql.impl.scanner.GraphQLTypeDefinitionRegistryScannerPlugin</class>
    </scanner>
</jpd:jqassistant-plugin>
